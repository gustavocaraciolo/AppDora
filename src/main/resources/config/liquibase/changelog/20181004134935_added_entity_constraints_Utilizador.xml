<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Utilizador.
    -->
    <changeSet id="20181004134935-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="utilizador"
                                 constraintName="fk_utilizador_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="utilizadors_id"
                                 baseTableName="utilizador_portal"
                                 constraintName="fk_utilizador_portal_utilizadors_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilizador"/>
        <addForeignKeyConstraint baseColumnNames="portals_id"
                                 baseTableName="utilizador_portal"
                                 constraintName="fk_utilizador_portal_portals_id"
                                 referencedColumnNames="id"
                                 referencedTableName="portal"/>
        
    </changeSet>
</databaseChangeLog>
